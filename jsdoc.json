{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/)node_modules\\/"
  },
  "opts": {
    "destination": "out/docs",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": ["./license-report.txt"]
      }
    }
  }
}
